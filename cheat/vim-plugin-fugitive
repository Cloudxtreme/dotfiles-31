# vim plugin fugitive

#fugitive git status
":Gstatus" <leader>gs

#fugitive Git add
toggle for "git add"

#fugitive Git add --patch
"p" for "git add --patch"

#fugitive Git commit
":Gcommit" <leader>gc

#fugitive Git Checkout --filename
":Gread"

#fugitive Git mv new_file_name
":Gmove"

#fugitive Git blame
":Gblame"

